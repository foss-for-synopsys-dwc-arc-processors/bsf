{
	"toolchain_linux_arc64": {
		"git": { "repo": "https://github.com/foss-for-synopsys-dwc-arc-processors/arc-gnu-toolchain", "branch": "master" },
		"execute": "$var(@SOURCE)/configure --prefix=$var(PREFIX) --target=arc64 --enable-linux ; make -j$var(NJOBS)",
		"version":"$var(VERSION)",
		"module_file": { "prefix":"$var(MODULE_PREFIX)", "template":"$var(TEMPLATE_PATH)/toolchain.module.erb" }
		
	},
	"qemu": {
		"git": { "repo": "https://github.com/foss-for-synopsys-dwc-arc-processors/qemu", "branch": "master" },
		"execute": "$var(@SOURCE)/configure --prefix=$var(PREFIX) --target-list=arc-softmmu --disable-werror --enable-debug ; make -j$var(NJOBS) ; make install",
		"version":"$var(VERSION)",
		"module_template":"$var(MODULE_PREFIX)/qemu.module.erb"
	}
}
